package com.iuvity.domain;

import java.io.Serializable;

public class RequestCreacionUsuario implements Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 1L;

	// ATRIBUTOS
	private String cedula;
	private String nombre;
	private String usuario;
	private String password;

	// CONSTRUCTORES
	/**
	 * Constructor con todos los campos
	 * 
	 * @param cedula
	 * @param nombre
	 * @param usuario
	 * @param password
	 */
	public RequestCreacionUsuario(String cedula, String nombre, String usuario, String password) {
		super();
		this.cedula = cedula;
		this.nombre = nombre;
		this.usuario = usuario;
		this.password = password;
	}

	/**
	 * Constructor vacio
	 */
	public RequestCreacionUsuario() {
		super();
	}

	// GETTERS AND SETTERS
	public String getCedula() {
		return cedula;
	}

	public void setCedula(String cedula) {
		this.cedula = cedula;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public String getUsuario() {
		return usuario;
	}

	public void setUsuario(String usuario) {
		this.usuario = usuario;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	// MÉTODOS
	/**
	 * Método toString
	 */
	@Override
	public String toString() {
		return "RequestCreacionCliente [cedula=" + cedula + ", nombre=" + nombre + ", usuario=" + usuario
				+ ", password=" + password + "]";
	}

}
