package com.iuvity.domain;

import java.io.Serializable;

public class RequestListarProductos extends Producto implements Serializable {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 1L;
	
	// Atributos
	private Float vlr_unitario;
	private Float saldo_cant;
	
	
	public RequestListarProductos(Integer codigo, String nombre, String referencia, String unidad, Float vlr_unitario,
			Float saldo_cant) {
		super(codigo, nombre, referencia, unidad);
		this.vlr_unitario = vlr_unitario;
		this.saldo_cant = saldo_cant;
	}


	public RequestListarProductos() {
	}
	
	
	
	
}
